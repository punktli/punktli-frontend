<div class="container container-sidebar mx-auto">
  <h1 class="main">Calendar</h1>
  <p>Welcome to your calendar my friend</p>
  <LinkTo @route="event.new.patient" class="btn btn-primary">New event</LinkTo>
  {{#each this.model as |event|}}
    <div>
      <LinkTo @route="event.details" @model={{event}}>{{event.name}}</LinkTo>
    </div>
  {{/each}}
</div>